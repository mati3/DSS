<!-- empleado BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Mon Nov 18 00:21:00 CET 2019 -->
<bpel:process name="empleado"
         targetNamespace="http://empleado"
         suppressJoinFailure="yes"
         xmlns:tns="http://empleado"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://www.w3.org/2001/XMLSchema">
         >

    <!-- Import the client WSDL -->
	<bpel:import location="empleadoArtifacts.wsdl" namespace="http://empleado" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:empleado"
                     myRole="empleadoProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:empleadoRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:empleadoResponseMessage"/>
        <bpel:variable name="Variable" type="ns1:string"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in empleado.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:empleado"
                 operation="getStatus" createInstance="yes"
		 variable="input"/>
        
        <!-- Generate reply to synchronous request -->
        
        <bpel:if name="If">
            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:name = "matilde"]]></bpel:condition>
            <bpel:assign validate="no" name="Assign">           
                
                <bpel:copy>
                    <bpel:from><bpel:literal xml:space="preserve"><tns:empleadoResponse xmlns:tns="http://empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:status>business</tns:status>
</tns:empleadoResponse></bpel:literal></bpel:from>
                    <bpel:to variable="output" part="payload"></bpel:to>
                </bpel:copy>
            </bpel:assign>
        <bpel:elseif>
                <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:name = "manuel"]]></bpel:condition>
                <bpel:assign validate="no" name="Assign1">
                    
                    
                    
                    <bpel:copy>
                        <bpel:from><bpel:literal xml:space="preserve"><tns:empleadoResponse xmlns:tns="http://empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:status>privado</tns:status>
</tns:empleadoResponse>
</bpel:literal></bpel:from>
                        <bpel:to variable="output" part="payload"></bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
            </bpel:elseif><bpel:else>
                <bpel:assign validate="no" name="Assign2">
                    
                    
                    
                    <bpel:copy>
                        <bpel:from><bpel:literal xml:space="preserve"><tns:empleadoResponse xmlns:tns="http://empleado" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:status>turista</tns:status>
</tns:empleadoResponse>
</bpel:literal></bpel:from>
                        <bpel:to variable="output" part="payload"></bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
            </bpel:else></bpel:if>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:empleado"
               operation="getStatus" variable="output"/>
    </bpel:sequence>
</bpel:process>

